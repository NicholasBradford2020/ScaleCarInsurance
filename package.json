{
  "name": "car-insurance-claims-system",
  "version": "1.0.0",
  "description": "AI-powered car insurance claims processing system with damage assessment, cost estimation, and repair shop management",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "echo \"No tests specified yet\" && exit 0",
    "test:watch": "echo \"No tests specified yet\" && exit 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf dist node_modules/.vite",
    "start": "npm run dev"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "lucide-react": "^0.263.1",
    "clsx": "^2.0.0",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.45.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "prettier": "^3.0.0"
  },
  "keywords": [
    "insurance",
    "claims",
    "ai",
    "damage-assessment",
    "react",
    "typescript",
    "tailwindcss",
    "automotive",
    "repair-shops",
    "cost-estimation"
  ],
  "author": "Car Insurance Claims System Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/car-insurance-claims-system.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/car-insurance-claims-system/issues"
  },
  "homepage": "https://github.com/your-username/car-insurance-claims-system#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
} 